{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2.5",
        "algolia/algoliasearch-client-php": "^2.6",
        "algolia/scout-extended": "^1.9",
        "appstract/laravel-blade-directives": "^1.8",
        "arcanedev/laravel-notes": "^7.0",
        "beyondcode/laravel-self-diagnosis": "^1.4",
        "calebporzio/parental": "^0.10.0",
        "calebporzio/sushi": "^2.0",
        "darkghosthunter/laraload": "^2.0",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^1.0",
        "guzzlehttp/guzzle": "^6.3",
        "iatstuti/laravel-cascade-soft-deletes": "^3.0",
        "laravel/framework": "^7.0",
        "laravel/sanctum": "^2.0",
        "laravel/scout": "^8.0",
        "laravel/telescope": "^3.1",
        "laravel/tinker": "^2.0",
        "larsjanssen6/underconstruction": "^6.0",
        "livewire/livewire": "^1.0",
        "mantas-done/laravel-apm": "^1.0",
        "mcamara/laravel-localization": "^1.5",
        "nuwave/lighthouse": "^4.10",
        "nwidart/laravel-modules": "^7.0",
        "predis/predis": "^1.1",
        "robertogallea/eloquent-api": "^1.1",
        "sentry/sentry-laravel": "^1.7",
        "spatie/laravel-activitylog": "^3.14",
        "spatie/laravel-medialibrary": "^8.0",
        "spatie/laravel-permission": "^3.11",
        "spatie/laravel-schemaless-attributes": "^1.7",
        "teamtnt/laravel-scout-tntsearch-driver": "^8.1"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "beyondcode/laravel-query-detector": "^1.2",
        "beyondcode/laravel-view-xray": "^1.3",
        "beyondcode/tinkerwell-helper": "^1.1",
        "facade/ignition": "^2.0",
        "fruitcake/laravel-telescope-toolbar": "^1.2",
        "fzaninotto/faker": "^1.9.1",
        "haydenpierce/class-finder": "^0.4.0",
        "laravel-shift/blueprint": "^1.4",
        "laravel/ui": "^2.0",
        "mbezhanov/faker-provider-collection": "^1.2",
        "mll-lab/laravel-graphql-playground": "^2.1",
        "mockery/mockery": "^1.3.1",
        "mtolhuys/laravel-schematics": "^0.10.3",
        "nunomaduro/collision": "^4.1",
        "nunomaduro/larastan": "^0.5.2",
        "nunomaduro/phpinsights": "^1.13",
        "phpunit/phpunit": "^9.0",
        "vemcogroup/laravel-translation": "^2.0",
        "wnx/laravel-stats": "^2.1"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan vendor:publish --force --tag=livewire:assets --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
